# Solana Bot Volume Configuration Template
# Copy this file to .env and fill in your values

# Required: Your private key (base58 encoded)
PRIVATE_KEY=

# Required: RPC endpoint for Solana connection
RPC_ENDPOINT=https://api.mainnet-beta.solana.com

# Required: WebSocket endpoint for Solana connection
RPC_WEBSOCKET_ENDPOINT=wss://api.mainnet-beta.solana.com

# Trading Configuration
IS_RANDOM=true
SWAP_ROUTING=false
DISTRIBUTION_AMOUNT=0.01
BUY_AMOUNT=0.001
BUY_UPPER_AMOUNT=0.005
BUY_LOWER_AMOUNT=0.001

# Timing Configuration (in milliseconds)
BUY_INTERVAL_MIN=1000
BUY_INTERVAL_MAX=3000

# Selling Configuration
SELL_ALL_BY_TIMES=10
SELL_PERCENT=80

# Wallet Configuration
DISTRIBUTE_WALLET_NUM=5
CHECK_BAL_INTERVAL=5000
WALLET_NUM=10

# Transaction Configuration
TX_FEE=1
ADDITIONAL_FEE=0.005

# Required: Token mint address to trade
TOKEN_MINT=

# Pool Configuration
POOL_ID=

# Logging
LOG_LEVEL=info

# Jito Configuration (optional)
JITO_KEY=
BLOCKENGINE_URL=
JITO_FEE=100000
